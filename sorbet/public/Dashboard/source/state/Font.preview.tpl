
<h2>Texture</h2>

<div class="scrollarea">
	<img src="{{preview}}">
</div>

<h2>Settings</h2>

<table class="fixed">
	<tr>
		<td>Charset</td>
		<td>{{charset}}</td>
	</tr>
	<tr>
		<td>Map</td>
		<td>{{map}}</td>
	</tr>
	<tr>
		<td>Baseline</td>
		<td>{{baseline}}</td>
	</tr>
	<tr>
		<td>LineHeight</td>
		<td>{{lineheight}}</td>
	</tr>
	<tr>
		<td>Kerning</td>
		<td>{{kerning}}</td>
	</tr>
	<tr>
		<td>Spacing</td>
		<td>{{spacing}}</td>
	</tr>
	<tr class="div"></tr>
	<tr>
		<td class="right" colspan="2">
			<button class="download" data-identifier="Font.fnt">Download Font.fnt</button>
		</td>
	</tr>
</table>

